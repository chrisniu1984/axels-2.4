## Axel settings, generated by configure
PREFIX=/usr/local
BINDIR=/usr/local/bin
ETCDIR=/usr/local/etc
SHAREDIR=/usr/local/share
MANDIR=/usr/local/share/man
LOCALE=/usr/local/share/locale

OUTFILE=axels
ARCH=Linux

DESTDIR=

all: i18n-mofiles
install: install-i18n
uninstall: uninstall-i18n
I18N=1
CC=gcc
STRIP=strip
CFLAGS=-D_LARGEFILE_SOURCE -D_FILE_OFFSET_BITS=64 -Os  -Wall
LFLAGS= -pthread -lssl
